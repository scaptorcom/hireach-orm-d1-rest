# Cloudflare D1 Configuration
# Copy this file to .env and fill in your actual values

# Your Cloudflare API token with D1 permissions
CLOUDFLARE_D1_TOKEN=your_cloudflare_api_token_here

# Your Cloudflare account ID
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Your D1 database ID
CLOUDFLARE_DATABASE_ID=your_database_id_here

# Your D1 database name
CLOUDFLARE_DATABASE_NAME=your_database_name_here